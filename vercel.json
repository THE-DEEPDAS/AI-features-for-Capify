{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "ml/financial_model.py",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "api/getAdvice.ts": {
      "runtime": "nodejs18.x",
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "buildCommand": "pip install -r requirements.txt --target pythonenv && npm run build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "env": {
    "PYTHONPATH": "/var/task/pythonenv:/var/lang/lib/python3.9/site-packages"
  }
}
